{
  "communication": {
    "defaultProvider": "provider-apollo",
    "providers": {
      "provider-apollo": {
        "type": "apollo",
        "baseUrl": "https://aw-unifi-graphql.netseven.it/"
      },
      "provider-rest": {
        "type": "rest",
        "baseUrl": "https://jsonplaceholder.typicode.com/",
        "defaultMethod": "GET"
      }
    }
  },
  "header": {
    "logo": {
      "image": "https://i.imgur.com/kTND3Do.png",
      "anchor": {
        "href": "home"
      }
    },
    "nav": {
      "items": [
        {
          "text": "Home",
          "icon": "n7-icon-home",
          "anchor": { "href": "home" },
          "_meta": {
            "id": "home"
          }
        },
        {
          "text": "Patrimonio",
          "anchor" : { "href": "patrimonio" },
          "icon": "n7-icon-tree-icon",
          "_meta": {
            "id": "patrimonio"
          }
        },
        {
          "text": "Galleria",
          "anchor" : { "href": "galleria" },
          "icon": "n7-icon-th",
          "_meta": {
            "id": "galleria"
          }
        },
        {
          "text": "Ricerca",
          "anchor" : { "href": "ricerca" },
          "icon": "n7-icon-search",
          "_meta": {
            "id": "ricerca"
          }
        },
        {
          "text": "Utenti",
          "anchor" : { "href": "utenti" },
          "icon": "n7-icon-facebook",
          "_meta": {
            "id": "utenti"
          }
        }
      ]
    },
    "menuToggle": {
      "open": {
        "payload": "mobile-open"
      },
      "close": {
        "payload": "mobile-close"
      }
    }
  },
