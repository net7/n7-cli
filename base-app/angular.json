{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "baseappprefix": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "baseappprefix",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "preserveSymlinks": true,
            "fileReplacements": [
              {
                "replace": "src/assets/app-config.json",
                "with": "src/assets/app-config.arianna.json"
              }, 
              {
                "replace": "src/app/app.module.ts",
                "with": "src/app/app.module.arianna.ts"
              }, 
              {
                "replace": "src/styles/styles.scss",
                "with": "src/styles/styles.arianna.scss"
              }
            ],
            "outputPath": "dist/baseappprefix",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/flatpickr/dist/flatpickr.min.css",
              "./node_modules/latte-carousel/dist/latte-carousel.min.css",
              "./node_modules/leaflet/dist/leaflet.css",
              "./node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "./node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "./node_modules/vis-timeline/dist/vis-timeline-graph2d.css",
              "src/styles/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "arianna": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/assets/app-config.json",
                  "with": "src/assets/app-config.arianna.json"
                }, 
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.module.arianna.ts"
                }, 
                {
                  "replace": "src/styles/styles.scss",
                  "with": "src/styles/styles.arianna.scss"
                }
              ]
            },
            "dataviz": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/assets/app-config.json",
                  "with": "src/assets/app-config.dataviz.json"
                }, 
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.module.dataviz.ts"
                }, 
                {
                  "replace": "src/styles/styles.scss",
                  "with": "src/styles/styles.dataviz.scss"
                }
              ]
            },
            "muruca": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/assets/app-config.json",
                  "with": "src/assets/app-config.muruca.json"
                }, 
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.module.muruca.ts"
                }, 
                {
                  "replace": "src/styles/styles.scss",
                  "with": "src/styles/styles.muruca.scss"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "baseappprefix:build"
          },
          "configurations": {
            "empty": {
              "browserTarget": "baseappprefix:build:empty"
            },
            "arianna": {
              "browserTarget": "baseappprefix:build:arianna"
            },
            "dataviz": {
              "browserTarget": "baseappprefix:build:dataviz"
            },
            "muruca": {
              "browserTarget": "baseappprefix:build:muruca"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "baseappprefix:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "baseappprefix:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "baseappprefix:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "baseappprefix"
}