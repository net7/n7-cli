{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "baseappprefix": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "baseappprefix",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "preserveSymlinks": true,
            "fileReplacements": [
              {
                "replace": "src/app/config",
                "with": "src/app/config.dataviz"
              },
              {
                "replace": "src/app/app.module.ts",
                "with": "src/app/app.module.dataviz.ts"
              },
              {
                "replace": "src/app/app.routes.ts",
                "with": "src/app/app.routes.dataviz.ts"
              },
              {
                "replace": "src/styles/styles.scss",
                "with": "src/styles/styles.dataviz.scss"
              }
            ],
            "outputPath": "dist/baseappprefix",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@teipublisher/pb-components/dist",
                "output": "/assets/pb-components/dist"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@teipublisher/pb-components/lib",
                "output": "/assets/pb-components/lib"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@teipublisher/pb-components/images",
                "output": "/assets/pb-components/images"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@teipublisher/pb-components/i18n",
                "output": "/assets/pb-components/i18n"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "./node_modules/flatpickr/dist/flatpickr.min.css",
              "./node_modules/tippy.js/dist/tippy.css",
              "./node_modules/tippy.js/themes/light-border.css",
              "./node_modules/latte-carousel/dist/latte-carousel.min.css",
              "./node_modules/@n7-frontend/icon-font/Font/n7-icon/style.css",
              "./src/styles/styles.scss",
              "./node_modules/leaflet/dist/leaflet.css",
              "./node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "./node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "./node_modules/vis-timeline/dist/vis-timeline-graph2d.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "dataviz": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/config",
                  "with": "src/app/config.dataviz"
                },
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.module.dataviz.ts"
                },
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.dataviz.ts"
                },
                {
                  "replace": "src/styles/styles.scss",
                  "with": "src/styles/styles.dataviz.scss"
                }
              ]
            },
            "arianna": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/config",
                  "with": "src/app/config.arianna"
                },
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.module.arianna.ts"
                },
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.arianna.ts"
                },
                {
                  "replace": "src/styles/styles.scss",
                  "with": "src/styles/styles.arianna.scss"
                }
              ]
            },
            "muruca": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/config",
                  "with": "src/app/config.muruca"
                },
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.module.muruca.ts"
                },
                {
                  "replace": "src/app/app.routes.ts",
                  "with": "src/app/app.routes.muruca.ts"
                },
                {
                  "replace": "src/styles/styles.scss",
                  "with": "src/styles/styles.muruca.scss"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "baseappprefix:build"
          },
          "configurations": {
            "arianna": {
              "browserTarget": "baseappprefix:build:arianna"
            },
            "dataviz": {
              "browserTarget": "baseappprefix:build:dataviz"
            },
            "muruca": {
              "browserTarget": "baseappprefix:build:muruca"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "baseappprefix:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "baseappprefix:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "baseappprefix:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "baseappprefix"
}
